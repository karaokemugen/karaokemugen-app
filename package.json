  "build": {
    "appId": "KaraokeMugen",
    "afterSign": "build/notarize.cjs",
    "npmRebuild": "false",
    "fileAssociations": [
      {
        "ext": "kmplaylist",
        "name": "Karaoke Mugen Playlist",
        "description": "Karaoke Mugen Playlist",
        "role": "Editor"
      },
      {
        "ext": "kmfavorites",
        "name": "Karaoke Mugen Favorites",
        "description": "Karaoke Mugen Favorites",
        "role": "Editor"
      }
    ],
    "nsis": {
      "warningsAsErrors": false,
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.md",
      "differentialPackage": false
    },
    "deb": {
      "depends": [
        "libnotify4",
        "libxtst6",
        "libnss3"
      ],
      "packageCategory": "video"
    },
    "dmg": {
      "icon": "build/DMG.icns",
      "contents": [
        {
          "x": 800,
          "y": 208,
          "name": "Karaoke Mugen.app"
        },
        {
          "x": 1000,
          "y": 208,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "icon1024.png",
      "compression": "normal",
      "publisherName": "Karaoke Mugen Team",
      "verifyUpdateCodeSignature": false,
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      }
    },
    "mac": {
      "artifactName": "${productName}-${version}-mac-${arch}.${ext}",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      },
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.plist",
      "entitlementsInherit": "build/entitlements.plist",
      "gatekeeperAssess": false,
      "compression": "normal",
      "minimumSystemVersion": "10.14.0",
      "category": "public.app-category.entertainment"
    },
    "linux": {
      "artifactName": "${productName}-${version}-linux-${arch}.${ext}",
      "target": [
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "appImage",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "vendor": "Karaoke Mugen Team",
      "executableName": "karaokemugen",
      "category": "AudioVideo",
      "synopsis": "Karaoke manager and player",
      "publish": {
        "provider": "generic",
        "url": "https://mugen.karaokes.moe/downloads/"
      }
    },
    "productName": "Karaoke Mugen",
    "copyright": "© 2017-2024 Karaoke Mugen Team",
    "extraResources": [
      "migrations/**",
      "locales/*",
      "assets/**",
      "kmfrontend/dist/**",
      "initpage/**",
      "build/icon1024.png",
      "build/electronAboutWindow.css"
    ],
    "extraFiles": [
      "app/bin/**",
      {
        "from": "app-linux/bin/${arch}",
        "to": "app/bin"
      },
      {
        "from": "app-mac/bin/${arch}",
        "to": "app/bin"
      },
      {
        "from": "app-mac/bin/universal",
        "to": "app/bin"
      }
    ],
    "directories": {
      "output": "packages",
      "buildResources": "build"
    },
    "files": [
      "dist/*",
      "package.json",
      "!**/node_modules/**/*"
    ],
    "protocols": [
      {
        "name": "km",
        "role": "Viewer",
        "schemes": [
          "km"
        ]
      }
    ]
  },
  "lint-staged": {
    "!(src/lib|assets/guestAvatars)": "prettier --write --ignore-unknown"
  },
  "packageManager": "yarn@1.22.22"
}
